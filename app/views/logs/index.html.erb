<h1>Logs</h1>

<table class="logs-table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Admin</th>
      <th>Item ID</th>
      <th>Content</th>
    </tr>
  </thead>
  <tbody>
    <% @logs.each do |l| %>
    <tr>
      <td><%= l.created_at %></td>
      <td><%= l.admin.name %></td>
      <td><%= l.item_id %></td>
      <td><%= l.content %></td>
    </tr>
    <% end %>
  </tbody>
</table>
